.container{
  height:100%;
  width:100%;
    font-family: "微软雅黑";
}
page{
  height:100%;
}
.inforBox{
  width:100%;
   background:white; 
  display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:center;
   -webkit-align-items:center;
}
.infor{
  width:100%;
  height:200rpx;
  background:white;
  display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:center;
  -webkit-align-items:center;
}
 .information,.couponBox{  
  width:680rpx;  
} 
.fenGe{
  background-color:#F2F2F2;  
  height:20rpx;
  width:100%;
}
.information{
  
  background:white;
  height:200rpx;
  border-bottom:1rpx solid #F2F2F2;
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
  align-items:center;
  -webkit-align-items:center;
  
}
.userinfo-avatar{
  width:140rpx;
  height:140rpx;
  background:green;
  border-radius:50%;
  /* background-size:cover; */
}
.information text{
  margin-left:30rpx;
}
.couponBox{   
    box-sizing: border-box;
  }

.coupon{
   padding-top:30rpx; 
  width:100%;
  height:400rpx;
  border-radius:18rpx;
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
   padding-left:20rpx;
  box-sizing: border-box;
  position:relative; 
    }
.coupon .logo{
  width:110rpx;
  height:110rpx;
  border-radius:50%;
  background:green;
}
.coupon .logo image{
  width:110rpx;
  height:110rpx;
}
.coupon .textBox{
  display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:left;
  -webkit-align-items:left;
  margin-left:20rpx;
   justify-content:space-between; 
    -webkit-justify-content:space-between; 
    height:100rpx;
  
}
.coupon .textBox .tea{
  font-size:12pt;
  margin:20rpx 0 0 20rpx;
  color:white;
}
.coupon .textBox .price{
  font-size:11pt;
  color:white;
}
 .coupon .card{
  width:160rpx;
  height:58rpx;
  border:1rpx solid #fff;
  border-radius:30rpx;
   position:absolute;
   right:20rpx;
   bottom:30rpx;
   text-align:center;
   
} 
.coupon .card text{
  font-size:11pt;
  color:white;
  line-height:58rpx;
}
.member text{
  font-size:9pt;
  padding:0;
  margin:0;
}
.member image{
  width:34rpx;
  height:34rpx;
  margin-right:14rpx;
  position:absolute;
  top:32rpx;
  right:140rpx;
}
.member{
   height:89rpx; 
  width:100%;
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
  align-items:center;
  -webkit-align-items:center;
  justify-content:flex-end;
  -webkit-justify-content:flex-end;
  position:relative;
 
}
.moneyBox{
  width:750rpx;  
  border-bottom:1rpx solid #F2F2F2;
}
.money{
   width:680rpx;   
  height:140rpx;
  margin:0 auto;
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
   align-items:center; 
   -webkit-align-items:center;
  text-align:center;
  position:relative;
  vertical-align: middle;

}
.money .consume{
  font-size:12pt;
}
.money .give{
  font-size:10pt;
  color:#FFCF75;
  margin-top:20rpx;
}
 .money .data{
  display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:flex-start;
  -webkit-align-items:flex-start;
  justify-content:center;
  -webkit-justify-content:center;
}
.money image{
  display:block;
   width:20rpx;
  height:30rpx; 
  position:absolute;
  right:0;
   top:55rpx;
}
