.container{
  height:100%;
  width:100%;
  background-color:#F2F2F2;  
  font-family: "微软雅黑";
}
page{
  height:100%;
  
}
.youngper{
  width:680rpx;
 border-radius:6rpx;
  background:#fff;
  margin:0 auto;
  margin-top:20rpx;
}
.youngper .xiaofei{
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
   align-items:center; 
   position:relative;
   border-bottom:1rpx solid #F2F2F2;
}
.youngper .orang{
  height:100rpx;
  background: #FF6738;
  width:6rpx;
}
.youngper .xiaofei text{
  font-size:12pt;
  margin-left:14rpx;
  color:#969696;
}
.youngper image{
  width:30rpx;
  height:20rpx;
  display:block;
  position:absolute;
  right:20rpx;
  top:40rpx;
}
.youhui{
  display:flex;
  display:-webkit-flex;
