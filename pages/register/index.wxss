.container{
  height:100%;
  width:100%;
  background-color:#F2F2F2;  
  font-family: "微软雅黑";
  display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:center;
}
page{
  height:100%;
}
.couponBox{
  width:680rpx; 
}


.couponBox{
  
   padding-top:30rpx; 
    box-sizing: border-box;
}

.coupon{
  width:100%;
  height:200rpx;
  border-radius:18rpx;
   display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
  align-items:center;
   padding-left:20rpx;
  box-sizing: border-box;
  position:relative; 
    }
.coupon .logo{
  width:110rpx;
  height:110rpx;
  border-radius:50%;
  background:green;
}
.coupon .logo image{
   width:110rpx;
  height:110rpx;
}
.coupon .textBox{
  display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:left;
   justify-content:space-between; 
  margin-left:20rpx;
  height:100rpx;
}
.coupon .textBox .tea{
  font-size:10pt;
  color:white;
}
.coupon .textBox .price{
  font-size:11pt;
  color:white;
}
 
.coupon .card text{
  font-size:11pt;
  color:white;
}
.formBox{
  width:680rpx; 
padding:0 20rpx;
  background:#fff;
  border-radius:18rpx;
  margin-top:30rpx;
  box-sizing:border-box;
  
}
.mob,.test{
  height:120rpx;
  }
.Kcard{
  height:150rpx;
}
.mob{
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
  align-items:center; 
  border-bottom:1rpx solid #F2F2F2; 
  position:relative;
   
}
.formBox .mob input{
  width:370rpx;  
  height:100rpx;
  margin-left:20rpx;
}
.container .formBox .mob .yanzheng1{
   position:absolute;
  right:0rpx;
   top:32rpx; 
   font-size:9pt;
   color:#fff;
    background:#727171;
   width:180rpx; 
    margin:0;  
  height:60rpx;
   line-height: 60rpx;  
}
  .formBox .mob .yanzheng2{
  position:absolute;
  right:0rpx;
   font-size:9pt;
   color:#fff;
  background:#FF6336;
   width:180rpx;
  padding:0;
  margin:0;
  top:32rpx;
  height:60rpx;
  line-height: 60rpx;
}  
.container .formBox .mob .yanzheng{
  position:absolute;
  right:0rpx;
   font-size:9pt;
   color:#fff;
   background:#FF9862;
  width:180rpx;
  padding:0;
  margin:0;
  top:32rpx;
  height:60rpx;
  line-height: 60rpx;
}
.formBox .mobstyle{
  font-size:12pt;
}
.formBox .test{
  display:flex;
  display:-webkit-flex;
  flex-direction:row;
  -webkit-flex-direction:row;
     align-items:center; 
     border-bottom:1rpx solid #F2F2F2; 
    
}
.formBox .test text{
  font-size:12pt;
}
.formBox .test input{
  width:520rpx;  
  height:100rpx;
    margin-left:20rpx; 
}
.formBox .btn{
  width:100%;
  height:90rpx;
  background:#727171;
  color:#fff;  
}
.formBox .btn1{
  width:100%;
  height:90rpx;
  background:#FF6336;
  color:#fff;  
}
.formBox .Kcard{
   display:flex;
  display:-webkit-flex;
  flex-direction:column;
  -webkit-flex-direction:column;
  align-items:center;  
  height:150rpx;  
  justify-content:center;
}
.myToast{
  width:240rpx;
  height:90rpx;
  line-height: 90rpx;
  border-radius:10rpx;
  background-color: rgb(114,113,113);
  color:rgb(255,255,255);
  font-size: 30rpx;
  text-align: center;
  position: absolute;
  left:255rpx;
  top:50%;
  z-index: 100;
  opacity: 0.85;
}
.technology{
	width:100%;
	font-size:8pt;
	color:#ccc;	
	text-align:center;
	padding-top:40rpx;
}